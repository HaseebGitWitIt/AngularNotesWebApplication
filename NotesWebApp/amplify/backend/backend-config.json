{
	"auth": {
		"authResource": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	},
	"storage": {
		"dynamoDbTableName": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"notesApiResource": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "dynamoDbTableName",
					"attributes": [
						"Name",
						"Arn"
					],
					"attributeEnvMap": {
						"Name": "TABLE_NAME",
						"Arn": "TABLE_ARN"
					}
				}
			]
		}
	},
	"api": {
		"notesAppApi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "notesApiResource",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}